<br/>
<!--Card class to display fields for user to fill in.-->
<div class="card">
  <div class="card-header">
    <h4>New Evaluation</h4>
  </div>
  <div class="card-body">
    <%= render 'form', evaluation: @evaluation, presentation: @presentation, presentation_id: @presentation.id %>
    <%= link_to 'Back', evaluations_path, class:"btn btn-secondary" %>
  </div>
</div>

